{
	"name": "AzureOutputFile",
	"properties": {
		"activities": [
			{
				"name": "lkp_sample2",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": "false",
					"secureInput": "false"
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": "true"
					},
					"sink": {
						"type": "BlobSink"
					},
					"enableStaging": "false",
					"stagingSettings": {
						"linkedServiceName": {
							"referenceName": "AzureBlobStorage4",
							"type": "LinkedServiceReference"
						},
						"path": "azureoutput"
					},
					"dataIntegrationUnits": 0,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"emp_id": "emp_id",
							"name": "name",
							"dept_code": "dept_code"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Sequential_File_0",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Sequential_File_0",
						"type": "DatasetReference"
					}
				]
			}
		]
	}
}