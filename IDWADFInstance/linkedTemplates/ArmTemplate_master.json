{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "idwadfinstance"
		},
		"AzureBlobStorage3_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage3'"
		},
		"AzureBlobStorage4_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage4'"
		},
		"AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase'"
		},
		"Dataset_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "Src_Employee.txt"
		},
		"Dataset_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "azureinput"
		},
		"Offices_Output_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "Src_Employee.txt"
		},
		"Offices_Output_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "azureinput"
		},
		"Offices_input_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "Src_Employee.txt"
		},
		"Offices_input_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "azureinput"
		},
		"Trial_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "Src_Employee.txt"
		},
		"Trial_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "azureinput"
		},
		"BlobTrigger_properties_Load JSON Blob To SQLDB - With Trigger_parameters_Filename": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"BlobTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/c799623a-3f04-4e86-b158-d98a2abd8139/resourceGroups/IDWIngestion/providers/Microsoft.Storage/storageAccounts/idwlake"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage3_connectionString": {
						"value": "[parameters('AzureBlobStorage3_connectionString')]"
					},
					"AzureBlobStorage4_connectionString": {
						"value": "[parameters('AzureBlobStorage4_connectionString')]"
					},
					"AzureSqlDatabase_connectionString": {
						"value": "[parameters('AzureSqlDatabase_connectionString')]"
					},
					"Dataset_properties_typeProperties_fileName": {
						"value": "[parameters('Dataset_properties_typeProperties_fileName')]"
					},
					"Dataset_properties_typeProperties_folderPath": {
						"value": "[parameters('Dataset_properties_typeProperties_folderPath')]"
					},
					"Offices_Output_properties_typeProperties_fileName": {
						"value": "[parameters('Offices_Output_properties_typeProperties_fileName')]"
					},
					"Offices_Output_properties_typeProperties_folderPath": {
						"value": "[parameters('Offices_Output_properties_typeProperties_folderPath')]"
					},
					"Offices_input_properties_typeProperties_fileName": {
						"value": "[parameters('Offices_input_properties_typeProperties_fileName')]"
					},
					"Offices_input_properties_typeProperties_folderPath": {
						"value": "[parameters('Offices_input_properties_typeProperties_folderPath')]"
					},
					"Trial_properties_typeProperties_fileName": {
						"value": "[parameters('Trial_properties_typeProperties_fileName')]"
					},
					"Trial_properties_typeProperties_folderPath": {
						"value": "[parameters('Trial_properties_typeProperties_folderPath')]"
					},
					"BlobTrigger_properties_Load JSON Blob To SQLDB - With Trigger_parameters_Filename": {
						"value": "[parameters('BlobTrigger_properties_Load JSON Blob To SQLDB - With Trigger_parameters_Filename')]"
					},
					"BlobTrigger_properties_typeProperties_scope": {
						"value": "[parameters('BlobTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}